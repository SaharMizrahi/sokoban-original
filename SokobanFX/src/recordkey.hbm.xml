<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="db.Level" table="Levels">
	<class name="db.Member" table="Members">

		<composite-id name=”key”>
		<key-property name="username" column="Members.username"  />
		<key-property name="levelId" column="Levels.LevelId" />
		
		</composite-id>

	</class>
</hibernate-mapping>